    chown root system /sys/class/sensors/bmi2xy_stepcnt/enable
    chown root system /sys/class/sensors/bmi2xy_stepcnt/poll_delay
    chown root system /sys/class/sensors/bmi2xy_stepcnt/uevent
    chown root system /sys/class/sensors/bmi2xy_stepcnt/flush
    chmod 660 /sys/class/sensors/bmi2xy_stepcnt/uevent
    chmod 770 /sys/class/sensors/bmi2xy_stepcnt/power

    chown root system /sys/class/sensors/bmi2xy_stepcnt_wakeup
