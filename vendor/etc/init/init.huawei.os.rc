    chmod 660 /sys/class/sensors/kx126-stepcnt-wakeup/uevent
    chmod 770 /sys/class/sensors/kx126-stepcnt-wakeup/power
	
    chown root system /sys/class/sensors/bma4xy_stationary_detect
    chown root system /sys/class/sensors/bma4xy_stationary_detect/enable
    chown root system /sys/class/sensors/bma4xy_stationary_detect/poll_delay
    chown root system /sys/class/sensors/bma4xy_stationary_detect/uevent
    chmod 660 /sys/class/sensors/bma4xy_stationary_detect/uevent
